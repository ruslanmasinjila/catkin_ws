				<mesh filename="package://lite6_omx/description/meshes/collision/link_base.stl"/>
			</geometry>
		</collision>

	</link>
	
	<!-- ############################# Link 1 ############################# -->
	
	<link name="link1">
	
	    <!--
		<inertial>
			<origin xyz="-0.00829544579053192 3.26357432323433E-05 0.0631194584987089" rpy="0 0 0" />
			<mass value="1.65393501783165" />
